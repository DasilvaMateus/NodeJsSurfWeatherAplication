{
  "name": "nodejssurfweatheraplication",
  "version": "1.0.0",
  "description": "Typescript Node.js WeatherApi",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint-fix": "eslint ./src ./test --ext .ts --fix",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "daSilvaMateus",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.14.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "config": "^3.3.12",
    "express": "^4.19.2",
    "module-alias": "^2.2.3"
  }
}
